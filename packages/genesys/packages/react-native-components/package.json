{
    "name": "@peersyst/react-native-components",
    "author": "Peersyst",
    "version": "1.2.6",
    "license": "MIT",
    "main": "./src/index.tsx",
    "engines": {
        "node": ">=10"
    },
    "scripts": {
        "build": "rm -rf ./build && yarn build:legacy && yarn build:modern && yarn build:node && yarn build:stable && yarn build:types && yarn build:copy-files",
        "build:legacy": "node ../../../../scripts/build legacy",
        "build:modern": "node ../../../../scripts/build modern",
        "build:node": "node ../../../../scripts/build node",
        "build:stable": "node ../../../../scripts/build stable",
        "build:types": "tsc --project .",
        "build:copy-files": "node ../../../../scripts/copy-files.js",
        "release": "yarn build && npm publish build",
        "generate-icons": "node ./scripts/iconGeneratorNative.js ./src/assets/svgs/ ./src/assets/icons/ && prettier --write ./src/assets/icons/"
    },
    "peerDependencies": {
        "@types/react": "~17.0.39",
        "@types/react-native": "~0.64.12",
        "expo": "^44.0.6",
        "react": "17.0.2",
        "react-dom": "17.0.2",
        "react-native": "0.64.3"
    },
    "peerDependenciesMeta": {
        "@types/react": {
            "optional": true
        },
        "@types/react-native": {
            "optional": true
        }
    },
    "dependencies": {
        "@babel/runtime": "^7.16.7",
        "@peersyst/react-components-core": "^0.2.2",
        "@peersyst/react-hooks": "^1.4.0",
        "@peersyst/react-native-modal": "^1.0.1",
        "@peersyst/react-native-styled": "^1.3.0",
        "@peersyst/react-native-utils": "^0.1.0",
        "@peersyst/react-types": "^1.4.0",
        "@peersyst/react-utils": "^1.6.0",
        "@react-native-async-storage/async-storage": "^1.17.7",
        "expo-barcode-scanner": "^11.2.1",
        "expo-camera": "~12.1.2",
        "expo-clipboard": "~2.1.0",
        "expo-constants": "~13.0.2",
        "expo-localization": "~12.0.0",
        "expo-status-bar": "^1.2.0",
        "react-native-gesture-handler": "~2.1.0",
        "react-native-keyboard-aware-scroll-view": "^0.9.5",
        "react-native-pager-view": "^5.4.15",
        "react-native-qrcode-svg": "^6.1.2",
        "react-native-safe-area-context": "3.3.2",
        "react-native-svg": "^12.1.1"
    },
    "devDependencies": {
        "@types/react": "~17.0.39",
        "@types/react-native": "~0.64.12",
        "expo": "^44.0.6",
        "react": "17.0.2",
        "react-dom": "17.0.2",
        "react-native": "0.64.3"
    },
    "publishConfig": {
        "access": "public"
    },
    "sideEffects": false
