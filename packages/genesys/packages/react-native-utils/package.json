{
    "name": "@peersyst/react-native-utils",
    "author": "Peersyst",
    "version": "0.3.1",
    "license": "MIT",
    "main": "./src/index.ts",
    "engines": {
        "node": ">=10"
    },
    "scripts": {
        "build": "rm -rf ./build && yarn build:legacy && yarn build:modern && yarn build:node && yarn build:stable && yarn build:types && yarn build:copy-files",
        "build:legacy": "node ../../../../scripts/build legacy",
        "build:modern": "node ../../../../scripts/build modern",
        "build:node": "node ../../../../scripts/build node",
        "build:stable": "node ../../../../scripts/build stable",
        "build:types": "tsc --project .",
        "build:copy-files": "node ../../../../scripts/copy-files.js",
        "release": "yarn build && npm publish build"
    },
    "peerDependencies": {
        "@types/react": "~17.0.39",
        "@types/react-native": "~0.64.12",
        "expo": "^44.0.6",
        "react": "17.0.2",
        "react-dom": "17.0.2",
        "react-native": "0.64.3"
    },
    "peerDependenciesMeta": {
        "@types/react": {
            "optional": true
        },
        "@types/react-native": {
            "optional": true
        }
    },
    "dependencies": {
        "@babel/runtime": "^7.16.7",
        "@peersyst/react-types": "^1.5.0"
    },
    "devDependencies": {
        "@types/react": "~17.0.39",
        "@types/react-native": "~0.64.12",
        "expo": "^44.0.6",
        "react": "17.0.2",
        "react-dom": "17.0.2",
        "react-native": "0.64.3"
    },
    "publishConfig": {
        "access": "public"
    },
    "sideEffects": false
}
